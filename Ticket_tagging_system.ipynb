        "!pip install -q cohere pandas\n",
        "\n",
        "import cohere, pandas as pd, time, json\n",
        "\n",
        "API_KEY = \"your-api"\n",
        "co = cohere.ClientV2(api_key=API_KEY) 
        "\n",
        "# 1ï¸âƒ£ Sample tickets\n",
        "tickets_df = pd.DataFrame([\n",
        "    {\"ticket_id\": \"T001\", \"subject\": \"Cannot login to my account\", \"description\": \"I've been trying to log in for the past hour but keep getting 'invalid credentials' error.\"},\n",
        "    {\"ticket_id\": \"T002\", \"subject\": \"Refund request for cancelled order\", \"description\": \"I cancelled my order #12345 three days ago but haven't received my refund yet.\"},\n",
        "    {\"ticket_id\": \"T003\", \"subject\": \"App keeps crashing on iPhone\", \"description\": \"The mobile app crashes every time I try to open the messages section. I'm using iPhone 12.\"},\n",
        "    {\"ticket_id\": \"T004\", \"subject\": \"How to upgrade my subscription plan?\", \"description\": \"I'm on the Basic plan and want to upgrade to Premium. Will I be charged immediately?\"},\n",
        "    {\"ticket_id\": \"T005\", \"subject\": \"Suspicious charge on my card\", \"description\": \"I noticed a charge of $49.99 on my card I don't recognize. Please investigate!\"},\n",
        "    {\"ticket_id\": \"T006\", \"subject\": \"Feature request: Dark mode\", \"description\": \"Please add a dark mode option for night-time usage.\"},\n",
        "    {\"ticket_id\": \"T007\", \"subject\": \"Email notifications not working\", \"description\": \"I'm not receiving email notifications even though enabled.\"},\n",
        "    {\"ticket_id\": \"T008\", \"subject\": \"Data export functionality broken\", \"description\": \"Exporting to CSV gives 'Export failed' error.\"},\n",
        "    {\"ticket_id\": \"T009\", \"subject\": \"Question about enterprise pricing\", \"description\": \"We're 50 employees, want enterprise pricing and volume discounts.\"},\n",
        "    {\"ticket_id\": \"T010\", \"subject\": \"Security concern: Unauthorized access\", \"description\": \"Login attempts from unknown IP in Russia. Please secure my account!\"}\n",
        "])\n",
        "\n",
        "# 2ï¸âƒ£ Categories\n",
        "CATEGORIES = [\"Account Access\",\"Billing & Payments\",\"Technical Issue\",\"Feature Request\",\"General Inquiry\",\"Security\",\"Bug Report\"]\n",
        "\n",
        "# 3ï¸âƒ£ Few-shot examples\n",
        "FEW_SHOT_EXAMPLES = [\n",
        "    {\"ticket\":\"I forgot my password and can't access my account\",\"tags\":[\"Account Access\",\"General Inquiry\"]},\n",
        "    {\"ticket\":\"The app freezes when I try to upload a file larger than 10MB\",\"tags\":[\"Technical Issue\",\"Bug Report\"]},\n",
        "    {\"ticket\":\"I was charged twice for the same subscription this month\",\"tags\":[\"Billing & Payments\",\"Technical Issue\"]}\n",
        "]\n",
        "\n",
        "# 4ï¸âƒ£ Tagging function using new Chat API\n",
        "def tag_ticket_cohere(ticket_text, categories, examples=None):\n",
        "    examples_text = \"\"\n",
        "    if examples:\n",
        "        for ex in examples:\n",
        "            examples_text += f'Ticket: \"{ex[\"ticket\"]}\"\\nTags: {\", \".join(ex[\"tags\"])}\\n'\n",
        "\n",
        "    system_msg = f\"You are an AI assistant that categorizes support tickets. Available categories: {', '.join(categories)}.\"\n",
        "    user_msg = f\"{examples_text}\\nTicket: \\\"{ticket_text}\\\"\\nTask: Return the top 3 relevant categories from the list above as JSON like:\\n{{\\\"tags\\\":[\\\"category1\\\",\\\"category2\\\",\\\"category3\\\"], \\\"reasoning\\\":\\\"Explain why these categories were chosen\\\"}}\"\n",
        "\n",
        "    try:\n",
        "        response = co.chat(\n",
        "      model=\"command-a-03-2025\",\n",
        "      messages=[\n",
        "          {\"role\":\"system\", \"content\": system_msg},\n",
        "          {\"role\":\"user\", \"content\": user_msg}\n",
        "      ]\n",
        "  )\n",
        "\n",
        "        text = response.message.content[0].text\n",
        "        if '{' in text:\n",
        "            text = text[text.find('{'): text.rfind('}')+1]\n",
        "            return json.loads(text)\n",
        "        else:\n",
        "            return {\"tags\":[\"General Inquiry\"], \"reasoning\":\"Parsing failed\"}\n",
        "    except Exception as e:\n",
        "        return {\"tags\":[\"General Inquiry\"], \"reasoning\":f\"Error occurred: {str(e)}\"}\n",
        "\n",
        "# 5ï¸âƒ£ Process all tickets\n",
        "def process_tickets(df, categories, examples=None):\n",
        "    results=[]\n",
        "    for _, row in df.iterrows():\n",
        "        ticket_text = f\"{row['subject']}. {row['description']}\"\n",
        "        res = tag_ticket_cohere(ticket_text, categories, examples)\n",
        "        results.append({\n",
        "            \"ticket_id\": row['ticket_id'],\n",
        "            \"subject\": row['subject'],\n",
        "            \"tag_1\": res['tags'][0] if len(res['tags'])>0 else None,\n",
        "            \"tag_2\": res['tags'][1] if len(res['tags'])>1 else None,\n",
        "            \"tag_3\": res['tags'][2] if len(res['tags'])>2 else None,\n",
        "            \"reasoning\": res['reasoning']\n",
        "        })\n",
        "        time.sleep(0.3)\n",
        "    return pd.DataFrame(results)\n",
        "\n",
        "# 6ï¸âƒ£ Run Zero-shot & Few-shot\n",
        "zero_shot_results = process_tickets(tickets_df, CATEGORIES)\n",
        "few_shot_results = process_tickets(tickets_df, CATEGORIES, FEW_SHOT_EXAMPLES)\n",
        "\n",
        "# 7ï¸âƒ£ Display results\n",
        "def display_results(df, title):\n",
        "    print(\"\\n\"+\"=\"*70)\n",
        "    print(title)\n",
        "    print(\"=\"*70)\n",
        "    for _, row in df.iterrows():\n",
        "        print(f\"\\nğŸ« {row['ticket_id']} | {row['subject']}\")\n",
        "        print(f\"ğŸ·ï¸  {row['tag_1']}, {row['tag_2']}, {row['tag_3']}\")\n",
        "        print(f\"ğŸ’¡ Reasoning: {row['reasoning']}\")\n",
        "    all_tags=[]\n",
        "    for c in ['tag_1','tag_2','tag_3']: all_tags.extend(df[c].dropna())\n",
        "    print(\"\\nTag Distribution:\\n\", pd.Series(all_tags).value_counts())\n",
        "\n",
        "display_results(zero_shot_results,\"ZERO-SHOT RESULTS\")\n",
        "display_results(few_shot_results,\"FEW-SHOT RESULTS\")\n",
        "\n",
        "# 8ï¸âƒ£ Compare\n",
        "print(\"\\n\"+\"=\"*70)\n",
        "print(\"ZERO-SHOT vs FEW-SHOT Comparison\")\n",
        "print(\"=\"*70)\n",
        "comp=[]\n",
        "for i in range(len(zero_shot_results)):\n",
        "    zs=zero_shot_results.iloc[i]; fs=few_shot_results.iloc[i]\n",
        "    comp.append({\n",
        "        \"ticket_id\": zs['ticket_id'],\n",
        "        \"zero_shot\": f\"{zs['tag_1']},{zs['tag_2']},{zs['tag_3']}\",\n",
        "        \"few_shot\": f\"{fs['tag_1']},{fs['tag_2']},{fs['tag_3']}\",\n",
        "        \"agreement\": \"Yes\" if zs['tag_1']==fs['tag_1'] else \"No\"\n",
        "    })\n",
        "comp_df=pd.DataFrame(comp)\n",
        "print(comp_df.to_string(index=False))\n",
        "print(f\"\\nPrimary Tag Agreement: {(comp_df['agreement']=='Yes').mean()*100:.1f}%\")\n"
      ],
